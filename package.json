        "archiver": "^6.0.0",
        "async-mutex": "^0.4.0",
        "await-exec": "^0.1.2",
        "aws-cron-parser": "^1.1.12",
        "axios": "^1.6.0",
        "babel-jest": "^29.7.0",
        "body": "^5.1.0",
