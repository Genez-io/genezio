{
  "name": "genezio",
  "version": "0.3.4",
  "description": "Command line utility to interact with Genezio infrastructure.",
  "main": "index.js",
  "scripts": {
    "start": "npx tsc && node build/index.js",
    "preinstall-locally": "npx tsc",
    "install-locally": "node solveEnvVariables.js build/variables.js",
    "postinstall-locally": "npm install -g",
    "build": "npx tsc && node solveEnvVariables.js build/variables.js",
    "install-locally-dev": "npm run install-locally -- dev",
    "build-dev": "npx tsc && node solveEnvVariables.js build/variables.js -- dev",
    "lint": "./scripts/lint",
    "checkChangesInVariables": "./scripts/checkVariables",
    "release": "npm run build && npm publish",
    "test": "jest"
  },
  "files": [
    "/build"
  ],
  "pre-commit": [
    "lint",
    "checkChangesInVariables"
  ],
  "author": "Genezio",
  "license": "GPL-3",
  "dependencies": {
    "@types/body": "^5.1.1",
    "@types/webpack-node-externals": "^2.5.3",
    "archiver": "^5.3.1",
    "aws-cron-parser": "^1.1.12",
    "axios": "^0.27.2",
    "body": "^5.1.0",
    "body-parser": "^1.20.1",
    "chokidar": "^3.5.3",
    "cli-spinner": "^0.2.10",
    "command-line-args": "^5.2.1",
    "commander": "^9.4.0",
    "cors": "^2.8.5",
    "cron-validator": "^1.3.1",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "http-terminator": "^3.2.0",
    "json5": "~2.2.3",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "moment": "^2.29.4",
    "node-abort-controller": "^3.1.1",
    "node-cron": "^3.0.2",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "open": "^8.4.0",
    "os": "^0.1.2",
    "parse-multipart-data": "^1.5.0",
    "path": "^0.12.7",
    "terminal-overwrite": "^2.0.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "unique-names-generator": "^4.7.1",
    "web3": "^1.8.0",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Genez-io/genezio.git"
  },
  "bin": {
    "genezio": "./build/index.js"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/cli-spinner": "^0.2.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.4",
    "@types/node-cron": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "envify": "^4.1.0",
    "eslint": "^8.24.0",
    "jest": "^29.2.2",
    "pre-commit": "^1.2.2",
    "ts-jest": "^29.0.3"
  },
  "engines": {
    "node": ">= 14"
  }
}
