{
  "name": "genezio",
  "version": "0.4.0",
  "description": "Command line utility to interact with Genezio infrastructure.",
  "main": "index.js",
  "scripts": {
    "start": "npx tsc && node build/index.js",
    "preinstall-locally": "npx tsc",
    "install-locally": "node solveEnvVariables.js build/constants.js",
    "postinstall-locally": "npm install -g",
    "build": "npx tsc && node solveEnvVariables.js build/constants.js",
    "install-locally-dev": "npm run install-locally -- dev",
    "build-dev": "npx tsc && node solveEnvVariables.js build/constants.js -- dev",
    "lint": "./scripts/lint",
    "checkChangesInVariables": "./scripts/checkConstants",
    "checkUnwantedLogs": "./scripts/checkUnwantedLogs",
    "release": "npm run build && npm publish",
    "test": "jest --testPathPattern tests"
  },
  "files": [
    "/build"
  ],
  "pre-commit": [
    "lint",
    "checkChangesInVariables",
    "checkUnwantedLogs"
  ],
  "author": "Genezio",
  "license": "GPL-3",
  "dependencies": {
